{
  "name": "beaverlabs/gg",
  "description": "PHP Debug, Logging Tools",
  "keywords": [
    "php",
    "debug",
    "logging",
    "beaverlabs",
    "gg"
  ],
  "homepage": "https://github.com/binbyz/gg",
  "license": "MIT",
  "version": "1.0.12",
  "authors": [
    {
      "name": "WONBEEN IM",
      "email": "eyedroot@gmail.com",
      "homepage": "https://twitter.com/eyedroot",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/support": "^7.20|^8.19|^9.0|^10.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.6.1",
    "phpunit/phpunit": "^10.1.3",
    "spatie/ray": "^1.37"
  },
  "autoload": {
    "psr-4": {
      "Beaverlabs\\Gg\\": "src/"
    },
    "files": [
      "src/helpers/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Beaverlabs\\Gg\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "funding": [
  ]
}
