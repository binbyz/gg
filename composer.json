{
  "name": "beaverlabs/gg",
  "description": "PHP Debug, Logging Tools",
  "keywords": [
    "php",
    "debug",
    "logging",
    "beaverlabs",
    "gg"
  ],
  "homepage": "https://github.com/binbyz/gg",
  "license": "MIT",
  "version": "0.1.0",
  "authors": [
    {
      "name": "WONBEEN IM",
      "email": "eyedroot@gmail.com",
      "homepage": "https://twitter.com/eyedroot",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "pestphp/pest": "^2.6.1"
  },
  "autoload": {
    "psr-4": {
      "Beaverlabs\\GG\\": "src/"
    },
    "files": [
      "src/helpers/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "funding": [
  ]
}
