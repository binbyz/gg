{
  "name": "beaverlabs/gg",
  "description": "PHP Debug, Logging Tools",
  "keywords": [
    "php",
    "debug",
    "logging",
    "beaverlabs",
    "gg"
  ],
  "homepage": "https://github.com/binbyz/gg",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "WONBEEN IM",
      "email": "eyedroot@gmail.com",
      "homepage": "https://twitter.com/eyedroot",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/log": "^7.20|^8.19|^9.0|^10.0",
    "illuminate/contracts": "^7.20|^8.19|^9.0|^10.0",
    "illuminate/database": "^7.20|^8.19|^9.0|^10.0",
    "illuminate/queue": "^7.20|^8.19|^9.0|^10.0",
    "illuminate/support": "^7.20|^8.19|^9.0|^10.0"
  },
  "require-dev": {
    "laravel/framework": "^7.20|^8.19|^9.0|^10.0",
    "orchestra/testbench-core": "^5.0|^6.0|^7.0|^8.5.8",
    "mockery/mockery": "^1.6.2",
    "pestphp/pest": "^2.6.1",
    "phpunit/phpunit": "^10.1.3"
  },
  "autoload": {
    "psr-4": {
      "Beaverlabs\\Gg\\": "src/"
    },
    "files": [
      "src/helpers/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Beaverlabs\\Gg\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Beaverlabs\\Gg\\Providers\\GgServiceProvider",
        "Beaverlabs\\Gg\\Providers\\EventServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "funding": [
  ]
}
